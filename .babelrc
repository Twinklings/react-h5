{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    "styled-jsx/babel",
    "inline-react-svg",
    "react-html-attrs",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": true
      }
    ]
  ]
}
